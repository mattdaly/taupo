{
    "name": "@taupo/taupo",
    "version": "0.0.0",
    "description": "A framework for building AI agents in Typescript",
    "license": "MIT",
    "sideEffects": false,
    "files": [
        "dist"
    ],
    "bin": {
        "taupo": "./dist/cli/index.mjs"
    },
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.cts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs"
        },
        "./cli": {
            "import": "./dist/cli/index.mjs",
            "require": "./dist/cli/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "scripts": {
        "build": "tsdown",
        "dev": "tsdown --watch",
        "test": "vitest",
        "typecheck": "tsc --build"
    },
    "dependencies": {
        "@taupo/ai": "*",
        "@taupo/lab": "*",
        "hono": "^4.6.14"
    },
    "devDependencies": {
        "@hono/node-server": "^1.13.7",
        "@taupo/tsconfig": "*",
        "@types/node": "^24.10.1",
        "ai": "^6.0.0-beta.99",
        "chokidar": "^4.0.3",
        "commander": "^12.1.0",
        "dotenv": "^17.2.3",
        "tsdown": "^0.16.0",
        "typescript": "^5.9.3",
        "vitest": "^4.0.8",
        "zod": "3.25.76"
    },
    "peerDependencies": {
        "ai": "^6.0.0-beta.99",
        "zod": "^3.25.76 || ^4.1.8"
    },
    "engines": {
        "node": ">=18"
    },
    "publishConfig": {
        "access": "public"
    }
}
